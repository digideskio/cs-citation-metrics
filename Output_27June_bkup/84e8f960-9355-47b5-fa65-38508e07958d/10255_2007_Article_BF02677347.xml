              <Heading>Key words</Heading>
              <Keyword>
                <Emphasis Type="Italic">K</Emphasis>
                <Subscript>1,3</Subscript>-free graph</Keyword>
              <Keyword>connected [2,3]-factor</Keyword>
            </KeywordGroup>
