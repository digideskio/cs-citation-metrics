              <Heading>Key words</Heading>
              <Keyword>Global attractor</Keyword>
              <Keyword>Hausdorff and fractal dimensions</Keyword>
              <Keyword>KdV equation</Keyword>
              <Keyword>weighted norm</Keyword>
            </KeywordGroup>
